---
import Menu from "./Menu.astro";
import Navigation from "./Navigation.astro";
import ThemeIcon from "./ThemeIcon.astro";
import H1 from "./atoms/H1.astro";

type Props = {
  appName: string;
};

const { appName } = Astro.props;
---

<header class="flex justify-between p-5">
  <H1 title={appName} />
  <ThemeIcon />
  <Menu />
  <Navigation />
</header>
